{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "warn",
    "space-before-function-paren": "off",
    "no-new": "off",
    "spaced-comment": "off",
    "comma-dangle": "off"
  }
}
